    "events": [
      {
        "name": "Form Submitted",
        "description": "fired when a user submits a form.",
        "rules": {
          "$schema": "http://json-schema.org/draft-07/schema#",
          "type": "object",
          "properties": {
            "context": {},
            "properties": {
              "required": [
                "form_id"
              ],
              "type": "object",
              "properties": {
                "form_id": {
                  "description": "the ID of the form",
                  "type": [
                    "string"
                  ]
                },
                "name": {
                    "description": "the name of the form",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "form_orientation": {
                    "description": "orientation of the form",
                    "type": [
                        "integer"
                    ]
                }
              }
            },
            "required": [
              "properties"
            ],
            "traits": {}
          }
        }
      }
    ],
